<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>

    <!-- Android 12+ -->
    <cloud-backup>
        <exclude domain="root" />
        <exclude domain="sharedpref" />
        <exclude domain="database" />
        <exclude domain="files" />
        <exclude domain="cache" />
    </cloud-backup>

    <device-transfer>
        <exclude domain="root" />
        <exclude domain="sharedpref" />
        <exclude domain="database" />
        <exclude domain="files" />
        <exclude domain="cache" />
    </device-transfer>

</data-extraction-rules>
